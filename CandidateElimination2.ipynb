""" 4. For a given set of training data examples implement and demonstrate the Candidate-Elimination algorithm to output a description
of the set of all hypotheses consistent with the training examples. """

def ok(x,h): return all(h[i]=='?' or h[i]==x[i] for i in range(len(x)))
def gen(h,x): return [h[i] if h[i]==x[i] else '?' for i in range(len(h))]

data=[
 ['Sunny','Warm','Normal','Strong','Yes'],
 ['Sunny','Warm','High','Strong','Yes'],
 ['Rainy','Cold','High','Strong','No'],
 ['Sunny','Warm','High','Weak','Yes']
]
attrs=[['Sunny','Rainy'],['Warm','Cold'],['Normal','High'],['Strong','Weak']]
S,G=[],[['?']*4]

print("EnjoySport Dataset:")
for i,e in enumerate(data): print(f"Example {i+1}: {e}")
print("\nInitial State:\nCurrent Version Space:\nS: EMPTY (no specific hypothesis)\nG1: <?,?,?,?>")

for i,e in enumerate(data):
 x,y=e[:-1],e[-1]
 print(f"\nStep {i+1}:\nProcessing: {x} -> {y}")
 if y=='Yes': G=[g for g in G if ok(x,g)]; S=[gen(S[0],x)] if S else [x]
 else:
  S=[s for s in S if not ok(x,s)]
  G=[g for g in G if not ok(x,g)] + [[g[j] if j!=i else v for j in range(4)]
     for g in G for i in range(4) for v in attrs[i] if g[i]=='?' and v!=x[i]]
 print("Current Version Space:")
 for i,s in enumerate(S): print(f"S{i+1}: <{','.join(s)}>")
 for i,g in enumerate(G): print(f"G{i+1}: <{','.join(g)}>")

print("\nFinal Version Space:\nCurrent Version Space:")
print(f"S1: <{','.join(S[0])}>")
for i,g in enumerate(G): print(f"G{i+1}: <{','.join(g)}>")
print("\nAnalysis:\nThe version space contains multiple hypotheses.")
print(f"Most specific: <{','.join(S[0])}>")
print(f"Most general: <{','.join(G[0])}>")
print(f"Total hypotheses in version space: {len(S)+len(G)}")

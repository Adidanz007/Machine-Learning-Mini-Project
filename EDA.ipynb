"""1. Develop a Python program to perform Exploratory Data Analysis (EDA) on the
California Housing dataset by generating histograms for all numerical features to
understand their distributions and box plots to visually detect and analyze the presence
of outliers."""

import pandas as pd
import matplotlib.pyplot as plt
from sklearn.datasets import fetch_california_housing

def load_data():
    data = fetch_california_housing()
    df = pd.DataFrame(data.data, columns=data.feature_names)
    df["MedHouseVal"] = data.target
    return df

def plot_histograms(df):
    df.hist(figsize=(15, 12), bins=30, edgecolor='black')
    plt.suptitle("Feature Distributions - California Housing", fontsize=16)
    plt.tight_layout()
    plt.show()

def plot_boxplots(df):
    plt.figure(figsize=(15, 12))
    outlier_info = {}

    for i, col in enumerate(df.columns, 1):
        plt.subplot(3, 3, i)
        df.boxplot(column=col)

        # Outlier calculation using IQR
        Q1, Q3 = df[col].quantile([0.25, 0.75])
        IQR = Q3 - Q1
        outliers = df[(df[col] < Q1 - 1.5 * IQR) | (df[col] > Q3 + 1.5 * IQR)]
        outlier_info[col] = len(outliers)

        plt.title(f"{col} ({len(outliers)} outliers)")

    plt.suptitle("Outlier Detection - Boxplots", fontsize=16)
    plt.tight_layout()
    plt.show()

    print("\nOutlier Summary:")
    for col, n in outlier_info.items():
        print(f"{col}: {n} outliers")

def main():
    print("Exploratory Data Analysis - California Housing")
    df = load_data()

    print(f"\nDataset Shape: {df.shape}")
    print("\nBasic Statistics:\n", df.describe())
    print("\nMissing Values:", df.isnull().sum().sum())

    print("\nGenerating Histograms...")
    plot_histograms(df)

    print("\nGenerating Boxplots and Detecting Outliers...")
    plot_boxplots(df)

    print("\nEDA Completed Successfully!")

if __name__ == "__main__":
    main()
